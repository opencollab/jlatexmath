               \setlength\fboxsep{1em}\fcolorbox{black}{#e6e6e6}{\text{\backslash{}fboxsep=1em}} \cr
               \setlength\fboxsep{2.5em}\fcolorbox{black}{#e6e6e6}{\text{\backslash{}fboxsep=2.5em}} \cr
               \setlength\fboxsep{3pt}\fcolorbox{black}{#e6e6e6}{\text{\backslash{}fboxsep=3pt}}
